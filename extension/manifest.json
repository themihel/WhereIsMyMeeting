{
  "name": "Where is my Meeting?",
  "version": "0.2.0",
  "description": "A small helper to find your online meetings (e.g. Google Meet) in your endless list of tabs!",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/whereismymeeting16.png",
      "32": "images/whereismymeeting32.png",
      "48": "images/whereismymeeting48.png",
      "128": "images/whereismymeeting128.png"
    }
  },
  "icons": {
    "16": "images/whereismymeeting16.png",
    "32": "images/whereismymeeting32.png",
    "48": "images/whereismymeeting48.png",
    "128": "images/whereismymeeting128.png"
  },
  "manifest_version": 3
}